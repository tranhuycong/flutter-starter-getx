    "version": "0.2.0",
    "configurations": [
        {
            "name": "flutter-starter-getx DEV",
            "request": "launch",
            "type": "dart",
            "args": [
                "--dart-define",
                "ENVIRONMENT=DEV",
            ]
        },
        {
            "name": "flutter-starter-getx PROD",
            "request": "launch",
            "type": "dart",
            "args": [
                "--dart-define",
                "ENVIRONMENT=PROD",
            ]
        },
        {
            "name": "flutter-starter-getx (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "flutter-starter-getx (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}